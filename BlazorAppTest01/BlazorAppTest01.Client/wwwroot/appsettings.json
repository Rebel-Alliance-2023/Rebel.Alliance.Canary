{
  "Oidc": {
    "Authority": "https://localhost:7095",
    "ClientId": "blazorcanary",
    "TokenEndpoint": "https://localhost:7095/connect/token",
    "ResponseType": "code",
    "DefaultScopes": [
      "openid",
      "profile",
      "email"
    ],
    "PostLogoutRedirectUri": "https://localhost:7095/",
    "RedirectUri": "https://localhost:7095/authentication/login-callback"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
